            <Pane layoutX="20.0" layoutY="20.0" prefHeight="560.0" prefWidth="660.0" style="-fx-background-color: #f1faee;">
               <children>
                  <Label alignment="CENTER" layoutX="133.0" layoutY="172.0" prefHeight="62.0" prefWidth="394.0" text="Клиент" textFill="#012551">
                     <font>
                        <Font name="DejaVu Sans Mono Bold" size="51.0" />
